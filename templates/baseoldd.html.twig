<body class="indent indent--rounded indent--shadowed indent--single-header">
<!-- PAGE WRAPPER -->


<div class="page page--w-header page--w-container">
    <!-- PAGE HEADER -->
    {% include("shared/header.html.twig") %}

    <!-- //END PAGE HEADER -->

    <!-- PAGE CONTAINER -->
    {#<div class="page page--w-header page--w-container" >#}
    {#<!-- MENU du haut -->#}
    {#{% block menu_haut %}#}
    {#{% include("shared/menu_haut.html.twig") %}#}
    {#{% endblock %}#}

    {#</div><!-- //END PAGE CONTAINER --><!-- PAGE CONTENT WRAPPER -->#}

    <!-- PAGE CONTAINER -->
    <div class="page__container invert">

        {#{% include "shared/menu_haut.html.twig" with {'liste_Droit_Profil' :  liste_Droit_Profils   }  %}#}
        {% include "shared/menu_haut.html.twig" %}
        <!-- MENU CONTEXTUEL -->
        {% block menu_contextuel %}
        {% endblock %}

    </div><!-- //END PAGE CONTAINER --><!-- PAGE CONTENT WRAPPER -->


    <!-- //END PAGE WRAPPER --><!-- TEMPLATE SETTINGS -->

    <div class="page__content page-aside--hidden" id="page-content">

        <!-- MENU PRINCIPAL -->
        {#{% block menu_gauche %}#}
        {% include("shared/menu.html.twig") %}
        {#{% endblock %}#}

        <!-- //END MENU PRINCIPAL -->


        <!-- PAGE CONTENT CONTAINER -->
        <div class="content" id="content"><!-- PAGE HEADING -->
            {% for message in app.session.flashbag.get('error') %}
                <div class="alert alert-danger alert-alt">
                    <i class="fa fa-exclamation fa-fw"></i>
                    <strong>{{ message|raw }}</strong>
                </div>
            {% endfor %}

            {% for message  in app.session.flashbag.get('success') %}
                <div class="alert alert-success alert-alt">
                    <i class="fa fa-thumbs-up fa-fw"></i>
                    {{ message|raw }}
                </div>
            {% endfor %}
            {% for message in app.session.flashbag.get('warning') %}
                <div class="alert alert-warning alert-alt">
                    <i class="fa fa-thumbs-up fa-fw"></i>
                    {{ message|raw }}
                </div>
            {% endfor %}
            <div class="page-heading">
                {% block pagetitle %}
                    <div class="page-heading__container">
                        <h1 class="title">Bienvenue sur </h1>
                        <p class="caption">Votre logiciel </p>
                    </div>
                    {#<div class="page-heading__container float-right d-none d-sm-block">#}
                        {#<a href="#" target="_blank"#}
                           {#class="btn btn-primary btn--icon btn--icon-stacked btn--anon d-none d-lg-block"><span#}
                                    {#class="text">$</span> ACTION</a>#}
                    {#</div>#}
                {% endblock %}

            </div><!-- //END PAGE HEADING -->
                <div class="container-fluid">
                    <div class="row">
                        {% block body %}


                        {% endblock %}
                    </div>

                    <div class="row">
                        {% block modals %}


                        {% endblock %}
                    </div>
                </div>
            </div><!-- //END PAGE CONTENT CONTAINER --></div><!-- //END PAGE CONTENT --></div>
     </div>
</div>
